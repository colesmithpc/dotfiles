hi Constant     guifg=#83a5d1
hi Identifier   guifg=#4588b8
hi Statement    guifg=#7e57c2
hi PreProc      guifg=#83a598
hi Type         guifg=#5f85d6
hi Special      guifg=#83a598
hi Underlined   guifg=#83a598
hi Todo         guifg=#fb4934 guibg=NONE gui=bold

